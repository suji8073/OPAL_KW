<?xml version="1.1" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    tools:remove="android:appComponentFactory"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.kw.opal">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"
        tools:ignore="CoarseFineLocation" />


    <application
        android:networkSecurityConfig="@xml/network_security_config"
        android:name=".GlobalApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        >

        <meta-data
            android:name="com.google.android.actions"
            android:resource="@xml/network_security_config" />

        <activity
            android:name="com.kakao.sdk.auth.AuthCodeHandlerActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />


                <data android:host="oauth"
                    android:scheme="@string/kakao_app_key" />
            </intent-filter>
        </activity>

        <activity android:name="com.kw.opal.routeinfo" android:label="route_inform"></activity>
        <activity android:name="com.kw.opal.login" android:label="login"></activity>
        <activity android:name="com.kw.opal.MainActivity" android:label="activity_main"></activity>
        <activity android:name="com.kw.opal.MainActivity_2" android:label="activity_main_2"></activity>
        <activity android:name="com.kw.opal.MainActivity_3" android:label="activity_main_3"></activity>
        <activity android:name="com.kw.opal.select1" android:label="select1"></activity>
        <activity android:name="com.kw.opal.area" android:label="area"></activity>
        <activity android:name="com.kw.opal.final_route_1" android:label="final_route_1"></activity>
        <activity android:name="com.kw.opal.final_route_2" android:label="final_route_2"></activity>
        <activity android:name="com.kw.opal.final_route_3" android:label="final_route_3"></activity>
        <activity android:name="com.kw.opal.tourism" android:label="tourism"></activity>
        <activity android:name="com.kw.opal.tourismselect" android:label="tourism2"></activity>
        <activity android:name="com.kw.opal.root_loading" android:label="root_loading"></activity>
        <activity android:name="com.kw.opal.random_1" android:label="random_1"></activity>
        <activity android:name="com.kw.opal.random_2" android:label="random_2"></activity>
        <activity android:name="com.kw.opal.root_making" android:label="root_making"></activity>
        <activity android:name="com.kw.opal.random_3" android:label="random_3"></activity>
        <activity android:name="com.kw.opal.random_3_play" android:label="random_3_play"></activity>
        <activity android:name="com.kw.opal.random_3_food" android:label="random_3_food"></activity>
        <activity android:name="com.kw.opal.random_3_sleep" android:label="random_3_sleep"></activity>
        <activity android:name="com.kw.opal.random_3_shop" android:label="random_3_shop"></activity>
        <activity android:name="com.kw.opal.smart_root" android:label="smart_root"></activity>
        <activity android:name="com.kw.opal.new_MainActivity_2" android:label="newmain2"></activity>
        <activity android:name="com.kw.opal.SaveRouteList" android:label="saveroutelist"></activity>
        <activity android:name="com.kw.opal.MainActivityAll" android:label="all_activity_main"></activity>




        <activity android:name="com.kw.opal.loading" android:exported="true">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>